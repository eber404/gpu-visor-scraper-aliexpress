{
  "name": "aliexpress-scraper-service",
  "version": "0.0.1",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc --project tsconfig.json",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint --fix src",
    "clean": "rm -rf node_modules yarn.lock package-lock.json dist yarn-error.log",
    "clean:install": "yarn clean && yarn --reset-cache",
    "test": "jest --config=jest.config.js --passWithNoTests",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@notionhq/client": "^2.0.0",
    "axios": "^1.1.3",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-fetch": "^3.1.5",
    "currency.js": "^2.0.4",
    "diff": "^5.1.0",
    "dotenv": "^16.0.1",
    "eventsource": "^2.0.2",
    "express": "^4.18.2",
    "fast-array-diff": "^1.0.1",
    "module-alias": "^2.2.2",
    "oxide.ts": "^1.0.5",
    "pino": "^8.7.0",
    "pocketbase": "^0.7.4",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.8.2",
    "tsyringe": "^4.7.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/helper-compilation-targets": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.6.0",
    "@swc/core": "^1.3.9",
    "@swc/helpers": "^0.4.12",
    "@types/diff": "^5.0.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "jest": "^29.2.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
